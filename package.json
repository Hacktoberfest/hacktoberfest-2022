  "dependencies": {
    "country-list": "^2.3.0",
    "html-react-parser": "^5.1.10",
    "knuth-shuffle": "^1.0.8",
    "markdown-it": "^13.0.2",
    "next": "14.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sitemap": "^7.1.1",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
